%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%%                         DEFINITION AV SATSER                               %%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\declaretheorem[name=Definition,
  refname={definition,definitions},
  Refname={Definition,Definitions},
  style=definition,
  numberwithin=section]{Def}
\declaretheorem[name=Example,
  refname={example,example},
  Refname={Example,Exampele},
  style=definition,
  numberwithin=section]{Ex}
\declaretheorem[name=Theorem,
  refname={theorem,theorems},
  Refname={Theorem,Theorems},
  numberlike=Def]{Thm}
\declaretheorem[name=Lemma,
  refname={lemma,lemman},
  Refname={Lemma,Lemman},
  numberlike=Thm]{Lemma}
\declaretheorem[name=Corollary,
  refname={Corollary,Corollary},
  Refname={Corollary,Corollary},
  numberlike=Thm]{Corr}
\declaretheorem[name=Remark,
  numbered=no,
  style=remark]{Remark}

